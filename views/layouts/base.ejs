<script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>


<script>
$(document).ready(function() {
  $('.select2').select2({
    theme: 'bootstrap-5',
    placeholder: 'Choisir une option',
    allowClear: true,
    width: '100%' // important pour le responsive
  });
});
</script>
</body>
</html>
